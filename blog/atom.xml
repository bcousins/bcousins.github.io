---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Ben Cousins' Blog</title>
	<link href="https://bdc.id.au/blog/atom.xml" rel="self"/>
	<link href="https://bdc.id.au/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://bdc.id.au/blog</id>
	<author>
		<name>Hank Quinlan</name>
		<email>hankquinlanhub@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://bdc.id.au{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://bdc.id.au{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>